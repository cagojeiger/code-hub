{
  "title": "Code-Hub Overview",
    "tags": ["codehub", "overview"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "piechart",
        "title": "Workspace 상태 분포",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_workspace_count_by_state{phase!~\"DELETED|DELETING\"}",
            "legendFormat": "{{ phase }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value"]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "type": "gauge",
        "title": "작업 성공률",
        "gridPos": {
          "x": 8,
          "y": 0,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(codehub_workspace_operations_total{status=\"success\"}[5m])) / sum(rate(codehub_workspace_operations_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true,
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 0.85,
                  "color": "yellow"
                },
                {
                  "value": 0.95,
                  "color": "green"
                }
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Coordinator 리더 상태",
        "gridPos": {
          "x": 16,
          "y": 0,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_coordinator_leader_status",
            "legendFormat": "{{ coordinator_type }}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value_and_name",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "No Leader",
                    "color": "red"
                  },
                  "1": {
                    "text": "Leader",
                    "color": "green"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 1,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "timeseries",
        "title": "상태 전환 추이 (분당)",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (from_state, to_state) (increase(codehub_workspace_state_transitions_total[1m])) * 60",
            "legendFormat": "{{ from_state }} → {{ to_state }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "unit": "tpm"
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "작업 실행 현황 (최근 1분)",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (operation, status) (increase(codehub_workspace_operations_total[1m]))",
            "legendFormat": "{{ operation }} ({{ status }})",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 30,
              "stacking": {
                "mode": "normal"
              }
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "작업 소요 시간 P95",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (operation, le) (rate(codehub_workspace_operation_duration_seconds_bucket[5m])))",
            "legendFormat": "{{ operation }} (P95)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "s",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "type": "bargauge",
        "title": "현재 상태별 워크스페이스 수",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_workspace_count_by_state",
            "legendFormat": "{{ phase }}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 100,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Coordinator Tick 소요 시간 P95",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (coordinator_type, le) (rate(codehub_coordinator_tick_duration_seconds_bucket[5m])))",
            "legendFormat": "{{ coordinator_type }} (P95)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 9,
        "type": "timeseries",
        "title": "Reconcile 큐 깊이",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_coordinator_wc_reconcile_queue",
            "legendFormat": "Queue Depth",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 20,
                  "color": "yellow"
                },
                {
                  "value": 50,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Coordinator Tick 에러율",
        "gridPos": {
          "x": 18,
          "y": 24,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (coordinator_type) (rate(codehub_coordinator_tick_total{status=\"error\"}[5m])) / sum by (coordinator_type) (rate(codehub_coordinator_tick_total[5m]))",
            "legendFormat": "{{ coordinator_type }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "percentunit",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 0.05,
                  "color": "yellow"
                },
                {
                  "value": 0.1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Observer API Latency P95",
        "gridPos": {
          "x": 0,
          "y": 32,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (resource_type, le) (rate(codehub_coordinator_observer_api_duration_seconds_bucket[5m])))",
            "legendFormat": "{{ resource_type }} (P95)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Observer API 에러 발생률",
        "gridPos": {
          "x": 12,
          "y": 32,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (resource_type, error_type) (rate(codehub_coordinator_observer_api_errors_total[5m]))",
            "legendFormat": "{{ resource_type }} - {{ error_type }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "mean"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 13,
        "type": "timeseries",
        "title": "WebSocket 활성 연결 수",
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_ws_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 14,
        "type": "timeseries",
        "title": "WebSocket 메시지 Latency P95",
        "gridPos": {
          "x": 8,
          "y": 40,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (direction, le) (rate(codehub_ws_message_latency_seconds_bucket[5m])))",
            "legendFormat": "{{ direction }} (P95)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["last", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "s",
            "decimals": 4
          }
        }
      },
      {
        "id": 15,
        "type": "timeseries",
        "title": "WebSocket 에러율",
        "gridPos": {
          "x": 16,
          "y": 40,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (error_type) (rate(codehub_ws_errors_total[5m]))",
            "legendFormat": "{{ error_type }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 16,
        "type": "timeseries",
        "title": "DB 연결 풀 사용률",
        "gridPos": {
          "x": 0,
          "y": 48,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_db_pool_checkedout / (codehub_db_pool_checkedout + codehub_db_pool_checkedin)",
            "legendFormat": "Pool Usage",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 0.7,
                  "color": "yellow"
                },
                {
                  "value": 0.9,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "type": "stat",
        "title": "DB 가용성",
        "gridPos": {
          "x": 12,
          "y": 48,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_db_up",
            "legendFormat": "Database",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "Disconnected",
                    "color": "red"
                  }
                }
              },
              {
                "type": "range",
                "options": {
                  "from": 1,
                  "to": 10,
                  "result": {
                    "text": "Connected",
                    "color": "green"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 1,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 18,
        "type": "stat",
        "title": "Circuit Breaker 상태",
        "gridPos": {
          "x": 0,
          "y": 56,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "codehub_circuit_breaker_state{name=\"external\"}",
            "legendFormat": "{{ name }}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "CLOSED",
                    "color": "green"
                  },
                  "1": {
                    "text": "HALF_OPEN",
                    "color": "yellow"
                  },
                  "2": {
                    "text": "OPEN",
                    "color": "red"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 2,
                  "color": "red"
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 19,
        "type": "timeseries",
        "title": "마지막 작업 실행 경과 시간",
        "gridPos": {
          "x": 8,
          "y": 56,
          "w": 16,
          "h": 8
        },
        "targets": [
          {
            "expr": "time() - codehub_workspace_last_operation_timestamp",
            "legendFormat": "{{ operation }}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "min": 0
          }
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
  "schemaVersion": 39,
  "version": 1,
  "uid": "codehub-overview",
  "overwrite": true
}
